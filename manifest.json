{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting", "webNavigation"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "host_permissions": ["<all_urls>"]
  }